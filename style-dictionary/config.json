{
	"source": ["./style-dictionary/properties/**/*.json"],
	"platforms": {
		"scss": {
			"transformGroup": "scss",
			"buildPath": "./src/design-tokens/",
			"prefix": "token",
			"files": [{
				"destination": "variables.scss",
				"format": "scss/variables"
			}]
		},
		"js": {
			"transformGroup": "js",
			"buildPath": "./src/design-tokens/",
			"files": [{
				"destination": "variables.js",
				"format": "javascript/es6"
			}]
		}
	}
}
